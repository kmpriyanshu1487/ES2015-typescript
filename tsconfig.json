{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "scripts",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ]
}
